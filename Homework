#Question1
color = ["blue", "black", "pink", "white"]
print(color)

#Question2
nums = list(range(30,60,3))
print(nums)
tuplenums = tuple(nums)
print(tuplenums)

#Question3
list = []
for i in range(6):
    list.append(i)
print(list)
list.remove(2)
print(list)
list.insert(1,2.0)
print(list)
print(len(list))
print(max(list))
print(min(list))

#Question4
a_list=[]
for number in range(1,11):
    a_list+=[number]
print(a_list)
sum(a_list)
